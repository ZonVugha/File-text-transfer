
    {{#if message}}
    <div style="text-align: center" class="alert alert-{{type}}">
      <button type="button" class="close" data-dismiss="alert">&times;</button>
      <strong>{{intro}}</strong> {{message}}
    </div>
    {{/if}}
<h1>{{title}}</h1>
